{
  "manifest_version": 3,
  "name": "CSRankings Enhance",
  "version": "0.2",
  "description": "Adds quick author lists (raw and adjusted) on csrankings.org",
  "content_scripts": [
    {
      "matches": [
        "*://csrankings.org/*",
        "*://www.csrankings.org/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "*://csrankings.org/*",
        "*://www.csrankings.org/*"
      ],
      "js": ["pageScript.js"],
      "run_at": "document_idle",
      "world": "MAIN"
    }
  ],
  "permissions": []
}
